{
  "name": "ooomobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "reinstall": "rm -rf node_modules && rm -rf ios/Pods && npm install && npx pod-install",
    "start": "react-native start",
    "postversion": "react-native-version",
    "test:static": "npm run check-types && npm run lint && npm run prettier:validate",
    "prettier": "prettier \"**/*.+(ts|tsx|json|graphql)\"",
    "prettier:format": "npm run prettier -- --write",
    "prettier:validate": "npm run prettier -- --list-different",
    "check-types": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "eslint 'src/**/*.ts' 'src/**/*.tsx' --fix",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/image-editor": "^2.3.0",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/core": "^5.12.1",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "formik": "^2.1.4",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "react-native-biometrics": "^2.1.4",
    "react-native-camera": "^3.35.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-permissions": "^2.1.5",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swipe-list-view": "^3.2.4",
    "react-native-vector-icons": "^7.0.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/node": "^13.11.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-perf": "^3.2.3",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "lint-staged": "^10.1.2",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "^2.0.4",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
